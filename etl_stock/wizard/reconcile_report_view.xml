<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        
        <record id="view_reconcile_report_form" model="ir.ui.view">
            <field name="name">Reconciliation Report</field>
            <field name="model">reconcile.report</field>
            <field name="arch" type="xml">
                <form string="Reconciliation Report">
                    <group>
                    	<group>
                    		<field name="start_date" required="1"/>
                    		<field name="categ_ids" options="{'no_create_edit':True, 'no_create':True,'no_open': True}"/>
                    	</group>
                    	<group>
                    		<field name="end_date" required="1"/>
                    	</group>
                    </group>
                    <footer>
                        <button name="action_print" string="Print" data-hotkey="q"
                            colspan="1" type="object" class="btn-primary"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_reconcile_report" model="ir.actions.act_window">
            <field name="name">Reconciliation Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">reconcile.report</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
       
       <menuitem id="menu_reconcile_report" parent="stock.menu_warehouse_report" action="action_reconcile_report" sequence="500"/>
       
    </data>
</odoo>    
